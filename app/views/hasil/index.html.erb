<div class="container-fluid">
    <div class="row kolom_hiji">
        <div class="col-lg-12 kata_persuasif judul_laporan">
            <h2>Laporan Respon Pengunjung</h2><br>
        </div>
        <div class="col-lg-4"></div>
        <div class="col-lg-4 kahandap_saetik_pisan">
            <div class= "kata_persuasif ketengahan_atuh">
                 <div class="ketengahan_atuh">
                    <p>
                        Statistik respon dari <%= @dataHasil.count %> pengunjung website ini :
                        <div id="masalah_manusia"></div>   
                    <p>
                </div>
                <br>
            </div>
        </div>
        <div class="col-lg-4">
           
        </div>
    </div>
</div>

<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script>
    // Load google charts
    google.charts.load('current', {'packages':['corechart']});
    google.charts.setOnLoadCallback(drawChart);

    // Draw the chart and set the chart values
    function drawChart() {
    var data = google.visualization.arrayToDataTable([
        ['Masalah', 'Jumlah'],
        ['Masalah 1', 8],
        ['Masalah 2', 8],
        ['Masalah 3', 8],
        ['Masalah 4', 8],
        ['Masalah 5', 8]
    ]);

    // Optional; add a title and set the width and height of the chart
    var options = {'width':550, 'height':400, 'backgroundColor': 'white'};

    // Display the chart inside the <div> element with id="piechart"
    var chart = new google.visualization.PieChart(document.getElementById('masalah_manusia'));
    chart.draw(data, options);
    }
</script>